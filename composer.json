{
  "name": "bonnier/wp-bonnier-redirect",
  "description": "Redirect for Bonnier integration with Custom Permalink and Polylang",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Bonnier Publications",
      "email": "interactive@bonnier.dk",
      "homepage": "http://bonnierpublications.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/BenjaminMedia/willow-mu-plugins"
    },
    {
      "type":"vcs",
      "url":"https://github.com/junaidbhura/composer-wp-pro-plugins"
    },
    {
      "type": "package",
      "package": {
        "name": "junaidbhura/advanced-custom-fields-pro",
        "version": "5.7.2",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://www.advancedcustomfields.com"
        },
        "require": {
          "junaidbhura/composer-wp-pro-plugins": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "junaidbhura/polylang-pro",
        "version": "2.3.8",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://www.polylang.pro"
        },
        "require": {
          "junaidbhura/composer-wp-pro-plugins": "*"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^5.6",
    "league/csv": "^9.1",
    "bonnier/contenthub-editor": "dev-IVD",
    "bonnier/wp-bonnier-cache": "dev-IVD"
  },
  "require-dev": {
    "johnpbloch/wordpress": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Bonnier\\WP\\Redirect\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
